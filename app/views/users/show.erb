<h1><%= @user.name %>'s projects:</h1>
    <% @user.projects.each do |project| %>
      <h3><%= project.project_name%></h3>
        <% project.tasks.each do |task| %>
        <% if task.completed %>
          <h5 style="text-decoration: line-through;"><%= task.task_name%><br></h5>
        <% else %>
          <h5><%= task.task_name%><br></h5>
        <% end %>
        <%= task.description%><br>
        Start date: <%= task.start_date%>
        End date: <%= task.end_date%></td>
        <br><br>

      <% end %>
      <br><br>
  <% end %>
